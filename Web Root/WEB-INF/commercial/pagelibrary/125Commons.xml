        <correction type="bestMatch" condition="incorrect" liberty="moderate" message="This value was changed to match an item in the list." />
      </fieldElement>
      <fieldElement type="text" id="Location.Addr.PostalCode" label="Zip Code" required="true" size="9" maxLength="10" downFillFieldId="LocationAddressZip" firstTimeDefaultValue="InsuredOrPrincipal.GeneralPartyInfo.Addr.PostalCode">
