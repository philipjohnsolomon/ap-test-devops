  <!-- Location -->
  <page id="location" source="Location" title="Locations" type="roster" downFillPageId="location">
    <pageElement type="tips">
      <fieldElement type="message">Enter all locations for any line(s) of business to be attached to this policy.</fieldElement>
    </pageElement>
    <pageElement id="location" type="roster" legend="Location Information" minEntries="1">
      <fieldElement type="column" id="Location.ItemIdInfo.AgencyId" label="Location #" maxLength="5" />
      <fieldElement type="column" id="Location" label="Location Address" maxLength="75" viewId="commonViews.xml:Display.Roster.Location" />
      <action type="edit" />
      <action type="delete" />
    </pageElement>
    <pageElement id="locationInformation" type="fieldset" legend="Location Information">
      <fieldElement type="hidden" id="Location.@id" label="Hidden Field" required="true" defaultValue="GUID()" copy="false" />
      <fieldElement type="hidden" id="Location.SubLocation.@id" label="Hidden Field" required="true" defaultValue="GUID()" copy="false" />
      <fieldElement type="hidden" id="Location.ItemIdInfo.SystemId" uniqueId="LocationSystemId" downFillFieldId="LocationSystemId" />
      <fieldElement type="text" id="Location.ItemIdInfo.AgencyId" maxLength="5" uniqueId="locationNumber" label="Location No." required="true">
			<validation type="numeric" />
			<validation type="minValue" >1</validation>
			<formatMask type="integer" />
      </fieldElement> 
      <fieldElement type="text" id="Location.Addr.Addr1" label="Address Line 1" required="true" size="30" maxLength="75" downFillFieldId="LocationAddressLine1"  firstTimeDefaultValue="InsuredOrPrincipal.GeneralPartyInfo.Addr.Addr1" />
      <fieldElement type="text" id="Location.Addr.Addr2" label="Address Line 2" size="30" maxLength="75" downFillFieldId="LocationAddressLine2" firstTimeDefaultValue="InsuredOrPrincipal.GeneralPartyInfo.Addr.Addr2" />
      <fieldElement type="text" id="Location.Addr.City" label="City" required="true" size="20" maxLength="50" downFillFieldId="LocationAddressCity" firstTimeDefaultValue="InsuredOrPrincipal.GeneralPartyInfo.Addr.City" />
