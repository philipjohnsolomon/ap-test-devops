										</xsd:sequence>
										<xsd:attribute name="id" type="ID"/>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="PhysicalDamage" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:sequence>
											<xsd:element ref="CommlCoverage" minOccurs="0" maxOccurs="unbounded"/>
