													<xsd:sequence>
														<xsd:element name="OperatorTypeCd" type="OpenEnum"/>
														<xsd:element ref="NumUnits"/>
													</xsd:sequence>
													<xsd:attribute name="id" type="ID"/>
												</xsd:complexType>
											</xsd:element>
										</xsd:sequence>
										<xsd:attribute name="LocationRef" type="IDREF"/>
										<xsd:attribute name="id" type="ID"/>
