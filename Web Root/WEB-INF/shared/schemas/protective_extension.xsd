			<xsd:element ref="PolicyTypeCd" minOccurs="0" maxOccurs="1"/>
			<xsd:element ref="ExecRecruitingIndustry" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
