		<eLabel>Vehicle_PrimaryLiabilityRatingFactor</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.PrimaryRatingFactorLiability</xpath>
	</field>
	<field>
		<eLabel>Vehicle_SeatingCapacityCount</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.SeatingCapacity</xpath>
	</field>
	<field>
		<eLabel>Vehicle_RadiusOfUse</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.RadiusUseTypeCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_FarthestZoneCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.FarZoneCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_CostNewAmount</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CostNewAmt.Amt</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Use_OtherIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement[VehUseCd='OT'].VehUseCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_Use_OtherDescription</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement[VehUseCd='OT'].VehUseCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_LiabilityIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='CSL'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_NoFaultIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='PIP'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_AdditionalNoFaultIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='APIP'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_MedicalPaymentsIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='MEDPM'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_UninsuredMotoristsIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='UMCSL_UMISP'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_UnderinsuredMotoristsIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='UNDSP'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_TowingAndLabourIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='TL'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_SpecifiedCauseOfLossIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='PERIL'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_FireIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='PFIRE'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_FireTheftIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='PFRTH'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_FireTheftWindstormIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='PTHWD'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_LimitedSpecifiedPerilsIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='LSPER'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_ComprehensiveIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='COMP'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_CollisionIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='COLL'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_RentalReimbursementIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='RREIM'].CoverageCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_FullGlassIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='COMP'].Option[OptionCd='FG'].OptionCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_ValuationActualCashValueIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='COMP'].Limit[ValuationCd='ACV'].ValuationCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_ValuationAgreedAmountIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='COMP'].Limit[ValuationCd='V'].ValuationCd</xpath>
	</field>
	<field type="checkbox">
		<eLabel>Vehicle_Coverage_ValuationStatedAmountIndicator</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='COMP'].Limit[ValuationCd='StatedAmt'].ValuationCd</xpath>
	</field>
