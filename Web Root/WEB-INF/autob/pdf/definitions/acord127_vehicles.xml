		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.VehSymbolCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_ComprehensiveSymbolCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.ComprehensiveOTCSymbolCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_CollisionSymbolCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CollisionSymbolCd</xpath>
	</field>
	<field indexRef="CommlAutoLineBusiness.CommlRateState.CommlVeh.@LocationRef">
		<eLabel>Vehicle_PhysicalAddress_LineOne</eLabel>
		<xpath>Location[@id='${indexRef}'].Addr.Addr1</xpath>
	</field>
	<field indexRef="CommlAutoLineBusiness.CommlRateState.CommlVeh.@LocationRef">
		<eLabel>Vehicle_PhysicalAddress_CityName</eLabel>
		<xpath>Location[@id='${indexRef}'].Addr.City</xpath>
	</field>
	<field indexRef="CommlAutoLineBusiness.CommlRateState.CommlVeh.@LocationRef">
		<eLabel>Vehicle_PhysicalAddress_CountyName</eLabel>
		<xpath>Location[@id='${indexRef}'].Addr.County</xpath>
	</field>
	<field indexRef="CommlAutoLineBusiness.CommlRateState.CommlVeh.@LocationRef">
		<eLabel>Vehicle_PhysicalAddress_StateOrProvinceCode</eLabel>
		<xpath>Location[@id='${indexRef}'].Addr.StateProvCd</xpath>
	</field>
	<field indexRef="CommlAutoLineBusiness.CommlRateState.CommlVeh.@LocationRef">
		<eLabel>Vehicle_PhysicalAddress_PostalCode</eLabel>
		<xpath>Location[@id='${indexRef}'].Addr.PostalCode</xpath>
	</field>
	<field>
		<eLabel>Vehicle_Registration_StateOrProvinceCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.Registration.StateProvCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_RatingTerritoryCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.TerritoryCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_GrossVehicleWeight</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.GrossVehOrCombinedWeight[UnitMeasurementCd='LBR'].NumUnits</xpath>
	</field>
	<field>
		<eLabel>Vehicle_RateClassCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.PrimaryClassCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_SpecialIndustryClassCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.SecondaryClassCd</xpath>
	</field>
