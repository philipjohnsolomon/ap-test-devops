		<eLabel>Vehicle_Coverage_ComprehensiveOrSpecifiedCauseOfLossDeductibleAmount</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd ='PERIL'  or CoverageCd ='PFIRE'  or CoverageCd='PFRTH' or CoverageCd ='PTHWD' or CoverageCd ='LSPER'].Deductible.FormatInteger</xpath>
	</field>
	<field>
		<eLabel>Vehicle_Collision_DeductibleAmount</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage[CoverageCd='COLL'].Deductible.FormatInteger</xpath>
	</field>
	<field>
		<eLabel>Vehicle_TotalPremiumAmount</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.FullTermAmt.Amt</xpath>
	</field>
	<checkBoxGroup>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.VehTypeCd</xpath>
		<checkBox trueValue="PP">
			<eLabel>Vehicle_VehicleType_PrivatePassengerIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="SPEC">
			<eLabel>Vehicle_VehicleType_SpecialIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="COML">
			<eLabel>Vehicle_VehicleType_CommercialIndicator</eLabel>
		</checkBox>
	</checkBoxGroup>
	<checkBoxGroup>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlVehSupplement.VehUseCd</xpath>
		<checkBox trueValue="PL">
			<eLabel>Vehicle_Use_PleasureIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="FM">
			<eLabel>Vehicle_Use_FarmIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="CM">
			<eLabel>Vehicle_Use_CommercialIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="RT">
			<eLabel>Vehicle_Use_RetailIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="SR">
			<eLabel>Vehicle_Use_ServiceIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="FH">
			<eLabel>Vehicle_Use_ForHireIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="DU">
			<eLabel>Vehicle_Use_UnderFifteenMilesIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="DO">
			<eLabel>Vehicle_Use_FifteenMilesOrOverIndicator</eLabel>
		</checkBox>
	</checkBoxGroup>
	<checkBoxGroup>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.CommlCoverage.CoverageCd</xpath>
		<checkBox trueValue="PERIL">
			<eLabel>Vehicle_Coverage_SpecifiedCauseOfLossDeductibleIndicator</eLabel>
		</checkBox>
		<checkBox trueValue="COMP">
			<eLabel>Vehicle_Coverage_ComprehensiveDeductibleIndicator</eLabel>
		</checkBox>
	</checkBoxGroup>
