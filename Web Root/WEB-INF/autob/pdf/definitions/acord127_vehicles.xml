<fields xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://reference.agencyport.com/schemas/5.2/pdf/pdfDefinitions.xsd">
	<field defaultValue="${itemIndex}">
		<eLabel>Vehicle_ProducerIdentifier</eLabel>
	</field>
	<field>
		<eLabel>Vehicle_ModelYear</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.ModelYear</xpath>
	</field>
	<field>
		<eLabel>Vehicle_ManufacturersName</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.Manufacturer</xpath>
	</field>
	<field>
		<eLabel>Vehicle_ModelName</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.Model</xpath>
	</field>
	<field>
		<eLabel>Vehicle_BodyCode</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.VehBodyTypeCd</xpath>
	</field>
	<field>
		<eLabel>Vehicle_VINIdentifier</eLabel>
		<xpath>CommlAutoLineBusiness.CommlRateState.CommlVeh.VehIdentificationNumber</xpath>
	</field>
	<field>
